package practisJava;

 class baseJava {

	void test1() {	System.out.println("Parent class method");}
		
}

 
 class childJava extends baseJava{

	void test2() {	System.out.println("child class method");}
		
}

public class PractisJava extends childJava{
	
	 void test3() {	System.out.println("Pactis class method");}
	
	public static void main (String Args[]){
		
		baseJava pj = new baseJava();
		baseJava p = new childJava();
		baseJava j = new PractisJava();
		pj.test1();
		p.test1();
		j.test1();
		
	}

}